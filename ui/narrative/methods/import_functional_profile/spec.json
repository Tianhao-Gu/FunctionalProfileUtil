      "advanced" : false
    },
    {
      "id": "organism_profile",
      "parameters": [
        "profile_name",
        "data_epistemology",
        "epistemology_method",
        "profile_file_path",
        "description"
      ],
      "optional": true,
      "allow_multiple": true,
      "with_border": false,
      "advanced" : false
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "FunctionalProfileUtil",
      "method" : "narrative_import_func_profile",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "narrative_system_variable": "workspace_id",
          "target_property": "workspace_id"
        },
        {
          "input_parameter" : "original_matrix_ref",
          "target_property" : "original_matrix_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "sample_set_ref",
          "target_property" : "sample_set_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "amplicon_set_ref",
          "target_property" : "amplicon_set_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "organism_profile",
          "target_property" : "organism_profile"
        },
        {
          "input_parameter" : "community_profile",
          "target_property" : "community_profile"
        },
        {
          "input_parameter" : "func_profile_obj_name",
          "target_property" : "func_profile_obj_name"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
